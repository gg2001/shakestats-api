// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Block {
  height       Int      @default(autoincrement()) @id
  hash         String
  createdAt    DateTime
  bids         Int
  openInterest Int
  transactions Int
  burned       Int?
  locked       Int?
  created      Int?
}

model Date {
  date         DateTime @id
  startHeight  Int
  bids         Int
  openInterest Int
  transactions Int
  burned       Int?
  locked       Int?
  created      Int?
}

model Name {
  name         String  @id
  hash         String
  bids         Int
  openInterest Int
  value        Int
  highestBid   Int
  claimed      Boolean @default(false)
  inUse        Boolean @default(false)
}